alias q=exit
alias e=exit
alias j=autojump
alias ls='ls -G'

alias ag='ag -U'

alias ta='tmux attach -t'
alias tl='tmux list-sessions'
alias tk='tmux list-panes -s -F '\''#{pane_pid} #{pane_current_command}'\'' | grep -v tmux | awk '\''{print $1}'\'' | xargs kill -9 ; e'
alias ts='tmux-start'
alias tr='tmux-resize'

alias c='tput reset'

# Git
alias gcd='git checkout develop'
alias gch='git checkout hotfix'
alias gcr='git checkout release'
alias gf='git fetch'
alias gs='git status'
alias gp='git push'
alias gl='git pull'
alias glu='git pull upstream'
alias ga='git add .'
alias go='git commit -m'
alias gc='git checkout'
alias gmd='git-merge-develop'
alias gbpurge='git branch --merged develop | grep -v "\*" | grep -v "master" | grep -v "develop" | grep -v "staging" | xargs -n 1 git branch -d'
alias gmodpull='git submodule foreach git pull origin master'

alias edf='vim -S $HOME/.vim_sessions/dotfiles.vim'

alias f='find . -name'

alias vi=vim

alias ctags-objc="ctags --languages=ObjectiveC --langmap=ObjectiveC:.h.m"
alias ctags-php="ctags --languages=PHP --langmap=PHP:.php"
alias ctags-js="ctags --languages=JavaScript --langmap=JavaScript:.js"
alias ctags-css="ctags --languages=css --langmap=css:.css"

alias phpcs='phpcs --standard=Symfony2'
